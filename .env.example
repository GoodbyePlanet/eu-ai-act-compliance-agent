ANTHROPIC_API_KEY=<your_api_key>
SERPAPI_API_KEY=<your_serpapi_api_key>
SERPER_API_KEY=<your_serper_api_key>
POSTGRES_USER=<postgres_user>
POSTGRES_PASSWORD=<postgres_password>
POSTGRES_DB=agent_sessions
DATABASE_URL=postgresql+asyncpg://<username>:<password>@localhost:5432/agent_sessions
BILLING_ENABLED=true
DAILY_FREE_CREDITS=20
GOOGLE_OIDC_AUDIENCE=<google_client_id>
GOOGLE_OIDC_ISSUER=https://accounts.google.com

# Docker/Traefik settings
APP_ENV_FILE=.env
TRAEFIK_IMAGE=traefik:v2.11
TRAEFIK_HTTP_PORT=80
TRAEFIK_DASHBOARD_ENABLED=false
TRAEFIK_UID_GID=0:0
DOCKER_SOCKET_PATH=/var/run/docker.sock
TRAEFIK_FRONTEND_RULE=PathPrefix(`/app`)
TRAEFIK_BACKEND_RULE=PathPrefix(`/api`)
